{
    "name": "emanci/version-comparator",
    "description": "Compares two version number strings based on Semantic Versioning 2.0.0",
    "keywords": ["version-compare"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [{
        "name": "emanci",
        "email": "zhengchaopu@gmail.com"
    }],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": {
            "Emanci\\VersionComparator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
